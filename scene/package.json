{
  "name": "dcl-project",
  "version": "1.0.0",
  "description": "SDK7 Template scene",
  "scripts": {
    "start": "sdk-commands start --web3",
    "deploy": "sdk-commands deploy",
    "deploy_kill": "sdk-commands deploy --target peer.kyllian.me",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next"
  },
  "devDependencies": {
    "@dcl/asset-packs": "1.11.0",
    "@dcl/js-runtime": "7.3.39",
    "@dcl/sdk": "7.3.39",
    "@types/text-encoding": "0.0.39"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "dependencies": {
    "@dcl-sdk/utils": "^1.2.3",
    "colyseus.js": "0.15.14",
    "core-js": "^3.34.0",
    "dcl-npc-toolkit": "^1.2.2",
    "dcl-ui-toolkit": "0.9.1",
    "text-encoding": "^0.7.0"
  },
  "bundleDependencies": [
    "@dcl-sdk/utils"
  ]
}
